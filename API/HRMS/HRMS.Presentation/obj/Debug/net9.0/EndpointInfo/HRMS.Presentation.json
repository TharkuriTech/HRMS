{
  "openapi": "3.0.1",
  "info": {
    "title": "HRMS.Presentation",
    "version": "1.0"
  },
  "paths": {
    "/api/Employee": {
      "post": {
        "tags": [
          "Employee"
        ],
        "operationId": "LoginEndPoint",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginCommand"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "409": {
            "description": "Conflict"
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginCommand": {
        "type": "object",
        "properties": {
          "strUserName": {
            "type": "string",
            "nullable": true
          },
          "strPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "isResult": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
    }
  }
}